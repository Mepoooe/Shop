footer.footer
	div.footer__logo
		a.footer__logo-link(href="index.html")
				h1.footer__logo-link-text
					|COOLTECH

	div.footer__navigation
		- var objectSocial = {'vk' : 'vk.com','fb' : 'fb.com','tw' : 'tw.com'}

			ul.footer__navigation-links-list
				each value, key in objectSocial
					li.footer__navigation-links-list-social-item
						a.footer__navigation-links-list-social(href= value)= key

		- var objectNav = {'Планшеты' : '#tablets','Мобильные телефоны' : '#mobile','Mp3 плееры' : '#pleers'}

				ul.footer__navigation-listOdersUp

					each value, key in objectNav
						a.footer__navigation-links(href= value)
							li.footer__navigation-links-item= key

		- var objectNav = {'Персональные компьютеры' : '#pc','Ноутбуки' : '#netbooks','Аксессуары' : '#accessories','Бытовая техника' : '#Appliances'}

				ul.footer__navigation-listOdersdown

					each value, key in objectNav
						a.footer__navigation-listOdersdown-links(href= value)
							li.footer__navigation-listOdersdown-links-item= key

	div.footer__copyright
		span.footer__copyright-text
			|All rights reserved. 2015. no copy alowed