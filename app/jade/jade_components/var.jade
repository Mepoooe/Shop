mixin products(name, list)
	li.product.product_view1.product_view2
		.product__box.product__box_view1.product__box_view2.product__box_img
			.product__slideshow
				.product__img-box.product__img-box_big
					img.product__img(src= list.img[0] alt=name)
				.product__img-line
					each item in list.img
						.product__img-box.product__img-box_mini
							img.product__img(src= item alt=name)
			.product__code
				span.product__code-label
					| Артикул&nbsp;
				= list.code
		.product__box.product__box_about
			h4.product__title
				span.product__title-type= list.type
				| &nbsp;
				= name
			.product__description
				.product__attr
					| Тип SIM-карты&nbsp;
					span.product__value= list.sim_type
				.product__attr
					| Количество SIM-карт&nbsp;
					span.product__value= list.sim_number
				.product__attr
					| Цвет&nbsp;
					span.product__value= list.color
				.product__attr
					| ОC&nbsp;
					span.product__value= list.os
				.product__attr
					| Связь 3G&nbsp;
					span.product__value= list.has3g
				.product__attr
					| 4G (LTE)&nbsp;
					span.product__value= list.has4g
				.product__attr
					| GPS-модуль&nbsp;
					span.product__value= list.gps
			.product__description-text= list.description
			ul.product__rating.unstyled-list
				li.product__star.product__star_red
				li.product__star.product__star_red
				li.product__star.product__star_red
				li.product__star.product__star_red
				li.product__star
				.rating= rating
		.product__box.product__box_buy
			.product__price= list.price
				span.product__price-label
					| p
			input.product__button.button(type='submit' value='В корзину')
			.product__availability= list.availability



mixin products_view1(name, list)
	li.product_view1
		.product__box_view1.product__box_view1_view1.product__box_view2_view1.product__box_img_view1
			.product__slideshow_view1
				.product__img-box_view1.product__img-box_big_view1
					img.product__img_view1(src= list.img[0] alt=name)
				.product__img-line_view1
					each item in list.img
						.product__img-box.product__img-box_mini
							img.product__img_view1(src= item alt=name)
			.product__code_view1
				span.product__code-label_view1
					| Артикул&nbsp;
				= list.code
		.product__box_view1.product__box_about_view1
			h4.product__title_view1
				span.product__title-type_view1= list.type
				| &nbsp;
				= name
			.product__description_view1
				
					span.product__value_view1= list.gps
			.product__description-text_view1= list.description
			ul.product__rating_view1.unstyled-list_view1
				li.product__star_view1.product__star_red_view1
				li.product__star_view1.product__star_red_view1
				li.product__star_view1.product__star_red_view1
				li.product__star_view1.product__star_red_view1
				li.product__star_view1
				.rating_view1= rating
		.product__box_view1.product__box_buy_view1
			.product__price_view1= list.price
				span.product__price-label_view1
					| p
			input.product__button_view1.button_view1(type='submit' value='В корзину')
			.product__availability_view1= list.availability




